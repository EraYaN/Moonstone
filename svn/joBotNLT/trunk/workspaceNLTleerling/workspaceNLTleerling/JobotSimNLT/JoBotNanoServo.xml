<muvium>
	<BOOTLOADER-VERSION>15</BOOTLOADER-VERSION>
	<DEVICE core="uVM-6722" />
	<CLOCK  f = "32" i="I" k="mhz" />
	<UART baud="115200" />
	<SLIP emulation="direct" duplex="full" packetsize="800" ipaddress="10.1.1.7" bootport="1" />
	<DEBUGGER enabled="false" psort="81" />
	<CLASS name="com.muvium.aptMuvium.SerialPortVirtualPortImpl">
		<PARAM name="include" value="False" /> 
	</CLASS>
	<CLASS name="com.muvium.aptMuvium.SerialPortVirtualImpl">
		<PARAM name="include" value="False" /> 
	</CLASS>
	<CLASS name="com.muvium.aptMuvium.SerialPortUARTPortImpl">
		<PARAM name="include" value="False" /> 
	</CLASS>
	<CLASS name="com.muvium.aptMuvium.SerialPortUARTImpl">
		<PARAM name="include" value="False" /> 
	</CLASS>
	<CLASS name="com.muvium.aptMuvium.MultiServoControllerPWMImpl">
		<PARAM name="include" value="false" /> 
	</CLASS>
	<CLASS name="com.muvium.aptMuvium.MultiServoControllerDirectImpl">
		<PARAM name="include" value="true" /> 
		<PARAM name="ServoSignalPins" value="G0G3E0E1E2" />
	</CLASS>
<CLASS name="com.muvium.driver.display.lcd.LCDPrinter">
	<PARAM name="LCD_E" value="D6" />
	<PARAM name="LCD_RW" value="D5" />
	<PARAM name="LCD_RS" value="D4" />
	<PARAM name="LCD_D4" value="D0" />
	<PARAM name="LCD_D5" value="D1" />
	<PARAM name="LCD_D6" value="D2" />
	<PARAM name="LCD_D7" value="D3" />
</CLASS>
<ADC-PORT configuration="All Analog" />
<UVMRUNNABLE-FILENAME>.\bin\javaBot\Nano\Servo\UVMDemo.class</UVMRUNNABLE-FILENAME>
</muvium>
